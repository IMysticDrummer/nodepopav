{
  "name": "nodepop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/allService",
    "startNodepop": "node ./bin/www",
    "startThumbnailer": "node ./microservice/thumbnailCreator.js",
    "dev": "DEBUG=nodepop:* npx nodemon ./bin/www",
    "devWin": "set DEBUG=nodepop:* & npx nodemon",
