    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "http-errors": "~1.6.3",
    "i18n": "^0.15.1",
    "jimp": "^0.16.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.5",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
